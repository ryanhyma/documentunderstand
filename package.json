{
  "name": "documentunderstand",
  "private": true,
  "version": "0.1.0",
  "description": "Monorepo for local OCR + LangGraph orchestration experiment",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "npm install",
    "start:orchestrator": "npm --workspace @doc/orchestrator run start",
    "dev:orchestrator": "npm --workspace @doc/orchestrator run dev"
  },
  "engines": {
    "node": ">=16"
  }
}
{
  "name": "documentunderstand",
  "version": "0.1.0",
  "description": "Local OCR + LangGraph orchestration experiment",
  "main": "src/orchestrator.js",
  "scripts": {
    "start": "node src/orchestrator.js",
    "dev": "nodemon src/orchestrator.js",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "tesseract.js": "^2.1.5",
    "langgraph": "^0.1.0"
  },
  "engines": {
    "node": ">=16"
  }
}
